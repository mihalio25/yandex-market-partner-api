# Пример файла конфигурации для Яндекс.Маркет API
# Скопируйте этот файл как config.env и заполните реальными данными

# Яндекс.Маркет API конфигурация
YANDEX_API_KEY=your_api_key_here
YANDEX_CAMPAIGN_ID=12345
YANDEX_BUSINESS_ID=67890

# Альтернативные кампании (если нужно переключиться)
YANDEX_CAMPAIGN_ID_EXPRES=54321

# Настройки по умолчанию
DEFAULT_BATCH_SIZE=10
DEFAULT_DELAY=1 